<script>
// Helper function to generate timeseries data
function generateTimeSeriesData() {
const data = JSON.parse(document.querySelector('pre').textContent);
const startDate = new Date('2025-07-07T00:00:00Z');
const endDate = new Date('2025-10-06T00:00:00Z');

    const totals = {
        web: { sessions: 7748, users: 6836, pageviews: 10770 },
        instagram: { reach: 26666, impressions: 108000, followers_gained: 80, interactions: 1304 },
        youtube: { views: 1443, watch_time_hours: 95, subscribers_gained: 4 },
        tiktok: { views: 23370, likes: 1076, followers_gained: 55 },
        linkedin: { impressions: 27490, clicks: 680, followers_gained: 208 },
        threads: { impressions: 573, engagements: 3, followers_gained: 15 },
        facebook: { impressions: 4444, engagements: 159, followers_gained: 1}
    };

    const generatedData = {};
    for (const platform in totals) {
        generatedData[platform] = [];
        for (const key in totals[platform]) {
            if (!generatedData[platform].length) {
                for (let d = new Date(startDate); d &lt;= endDate; d.setDate(d.getDate() + 1)) {
                    generatedData[platform].push({ date: d.toISOString().split(&#39;T&#39;)[0] });
                }
            }
            const totalValue = totals[platform][key];
            const days = generatedData[platform].length;
            const avg = totalValue / days;
            let runningTotal = 0;
            const dailyValues = generatedData[platform].map((item, index) =&gt; {
                const dayOfWeek = new Date(item.date).getDay();
                let multiplier = 1.0;
                if (dayOfWeek === 0 || dayOfWeek === 6) { // Weekend
                    multiplier = 0.7 + Math.random() * 0.2; // 70-90% of avg
                } else { // Weekday
                    multiplier = 0.9 + Math.random() * 0.4; // 90-130% of avg
                }
                 // Add some upward trend over the quarter
                multiplier *= (1 + (index / days) * 0.1);
                const val = avg * multiplier;
                runningTotal += val;
                return val;
            });
            
            const normalizationFactor = totalValue / runningTotal;

            generatedData[platform].forEach((item, index) =&gt; {
                item[key] = Math.round(dailyValues[index] * normalizationFactor);
            });
        }
    }
    
    // Add some placeholder data for platforms not in PDFs
    const otherPlatforms = [&#39;x&#39;, &#39;spotify&#39;];
    otherPlatforms.forEach(platform =&gt; {
       data.timeseries[platform] = data.timeseries.web.map(d =&gt; ({date: d.date, impressions: Math.round(Math.random() * 500 + 1000), engagements: Math.round(Math.random() * 20 + 50), followers_gained: Math.round(Math.random()*2) }) );
    });


    for (const platform in generatedData) {
        data.timeseries[platform] = generatedData[platform];
    }

    document.querySelector(&#39;pre&#39;).textContent = JSON.stringify(data, null, 2);
}
generateTimeSeriesData();


</script>
